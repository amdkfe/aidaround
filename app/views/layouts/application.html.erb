<!DOCTYPE html>
<html>
  <head>
    <title>AidAround</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    
    <%= javascript_include_tag "https://maps.google.com/maps/api/js?key=AIzaSyDs19RgEtMAbQpTzxQsgVSV_RBQaLP1MrY", 'application', 'data-turbolinks-track': 'reload' %>

    <!--<script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyDs19RgEtMAbQpTzxQsgVSV_RBQaLP1MrY"></script> -->    
    <!-- <script src="https://maps.googleapis.com/maps/api/geocode/json?key=AIzaSyDs19RgEtMAbQpTzxQsgVSV_RBQaLP1MrY"></script> -->
 
    <script src="//cdn.rawgit.com/mahnunchik/markerclustererplus/master/dist/markerclusterer.min.js"></script>  
    <script src='//cdn.rawgit.com/printercu/google-maps-utility-library-v3-read-only/master/infobox/src/infobox_packed.js' type='text/javascript'></script>
    <script src='//underscorejs.org/underscore-min.js' type='text/javascript'></script>

  </head>

  <body>
    <!-- navbar elements -->
    <div class="navbar">
      <ul class="logo">
        <%= render 'navbar/logo' %>
      </ul>
      <ul class="deviselink">
       <%= render 'navbar/login_join' %>
      </ul>
    </div>
 
    <!--  alert box -->
    <% flash.each do |key, value| %>
      <div class="<%= flash_class(key) %>">
            <%= value %>
      </div>
    <% end %>
 

    <%= yield %>

  </body>
</html>
